Xft.dpi:                        96
URxvt*termName:                 rxvt-256color
URxvt*font:                     xft:Inconsolata:size=16
URxvt.depth:                    0
URxvt*lineSpace:                1
URxvt.letterSpace:              -1
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt*geometry:                 80
URxvt.scrollBar:                false
